<h1>Listing projects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th>Area</th>
      <th>Stage</th>
      <th>Tmcp rep</th>
      <th>Bm rep</th>
      <th>Priority</th>
      <th>Fih target</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.name %></td>
        <td><%= project.code %></td>
        <td><%= project.area %></td>
        <td><%= project.stage %></td>
        <td><%= project.tmcp_rep %></td>
        <td><%= project.bm_rep %></td>
        <td><%= project.priority %></td>
        <td><%= project.fih_target %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path %>
